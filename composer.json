{
  "name": "passle/passle-sync",
  "type": "wordpress-plugin",
  "require": {
    "woocommerce/action-scheduler": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Passle\\PassleSync\\": "class/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "require-dev": {
    "phpdocumentor/shim": "3",
    "saggre/phpdocumentor-markdown": "^0.1.3"
  },
  "config": {
    "allow-plugins": {
      "phpdocumentor/shim": true
    }
  },
  "scripts": {
    "generate-docs": "php ./vendor/bin/phpdoc"
  }
}
